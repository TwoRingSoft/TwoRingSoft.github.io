---

<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
	<channel>
		<title>Two Ring Software RSS</title>
		<description>Two Ring Software.</description>
		<link>http://tworingsoft.com/</link>
		<link rel="alternate" type="text/html" href="http://tworingsoft.com/" />
		<link rel="self" type="application/atom+xml" href="http://tworingsoft.com/feed.xml" />
		<id>http://tworingsoft.com/feed.xml</id>
		{% for post in site.posts %}
			<item>
				<title>{{ post.title | xml_escape }}</title>
				<description>{{ post.abstract | xml_escape }}</description>
				<pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
				<link>{{ post.url | prepend: site.url }}</link>
				<guid isPermaLink="true">{{ post.url | prepend: site.url }}</guid>
				{% if post.thumbnail != undefined %}
				<media:content 
				    xmlns:media="http://tworingsoft.com" 
				    url="http://tworingsoft.com/blog/img/{{ post.thumbnail }}" 
				    medium="image" 
				    type="image/jpeg" 
				    width="150" 
				    height="150" />
				{% endif %}
			</item>
		{% endfor %}
	</channel>
</rss>
	