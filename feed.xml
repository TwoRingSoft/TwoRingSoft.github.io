<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<atom:link href="http://tworingsoft.com/feed.xml" rel="self" type="application/rss+xml" />
		<title>Two Ring Software RSS</title>
		<description>Two Ring Software.</description>
		<link>http://tworingsoft.com/</link>
		{% for post in site.posts %}
			<item>
				<title>{{ post.title | xml_escape }}</title>
				<subtitle>{{ post.abstract | xml_escape }}</subtitle>
				<updated>{{ post.date | date_to_rfc822 }}</updated>
				<link rel="related" type="text/html" href="{{ post.url | prepend: site.url }}" />
				<guid isPermaLink="true">{{ post.url | prepend: site.url }}</guid>
				{% if post.thumbnail != undefined %}
				<media:content 
				    xmlns:media="http://tworingsoft.com" 
				    url="http://tworingsoft.com/blog/img/{{ post.thumbnail }}" 
				    medium="image" 
				    type="image/jpeg" 
				    width="150" 
				    height="150" />
				{% endif %}
			</item>
		{% endfor %}
	</channel>
</rss>
	